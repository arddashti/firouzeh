              "//            'DimTimeTable'[Date],",
              "//            BLANK(),",
              "//            CALCULATE(LASTDATE('DimTimeTable'[Date]))",
              "//        ),",
              "//        CALCULATE(COUNT('FactPortfolio'[ShareNo]))",
              "//    )",
              "//)",
              "//",
              "RETURN",
              "CALCULATE(",
              "    SUM('FactPortfolio'[ShareNo]), ",
              "    'DimTimeTable'[Date] = LastDay,",
              "    --'DimInstrument'[InstrumentId] = Instrument",
              "    FILTER('FactPortfolio', B)",
              ")",
              "",
              ""
            ],
            "formatString": "#,0"
          },
          {
            "name": "Portfolio Avg Cost",
            "expression": [
              "",
              "VAR LastDay = ",
              "CALCULATE(",
              "    LASTNONBLANK(",
              "        'DimTimeTable'[Date],",
              "        COUNTROWS(RELATEDTABLE('FactPortfolio'))",
              "    ),",
              "    ALL('FactPortfolio'[AssetId]),",
              "    All('FactPortfolio'[InstrumentId]),",
              "    All('DimAsset'),",
              "    All('DimInstrument')",
              ")",
              "",
              "RETURN",
              "CALCULATE(",
              "    SUMX(",
              "        'FactPortfolio',",
              "        'FactPortfolio'[AVGBuyPrice] * 'FactPortfolio'[ShareNo]",
              "    ), 'DimTimeTable'[Date] = LastDay)",
              ""
            ],
