              "VAR r =",
              "    CALCULATE (",
              "        AVERAGE ( 'FactFunds'[6MonthReturn] ),",
              "        'DimTimeTable'[Date] = MaxDate,",
              "        NOT ( ISBLANK ( 'FactFunds'[6MonthReturn] ) ),",
              "        'FactFunds'[6MonthReturn] <> 0",
