              "    )",
              ")",
              "",
              "VAR T2 = ",
              "FILTER(",
              "    T1,",
              "    [Rank] <= NumToShow",
              ")",
              "        ",
              "RETURN",
              "IF(",
              "    ISFILTERED('FactReturns'[Description]),",
              "    CONCATENATEX(",
              "        T2,",
              "        'FactReturns'[Description],",
              "        \", \"",
              "    ) &",
              "    IF(COUNTROWS(T1) > NumToShow, \", etc\"),",
              "    \" - \" & \"All Funds -\"",
              ")",
              ""
            ]
          },
