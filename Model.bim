              "RETURN",
              "IF(",
              "    ISFILTERED('DimFunds'[FundType]),",
              "    CONCATENATEX(",
              "        T2,",
              "        'DimFunds'[FundTypeEn],",
              "        \", \"",
              "    ) &",
              "    IF(COUNTROWS(T1) > NumToShow, \", etc\"),",
              "    \" - \" & \"All Funds -\"",
              ")",
              ""
            ]
          },
