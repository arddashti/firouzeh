            "expression": [
              " ",
              "VAR LastTradeDay = MAX('FactTradeDetails'[Date])",
              "RETURN ",
              "SWITCH(",
              "    TRUE(),",
              "    'DimTimeTable'[Date] = LastTradeDay, \"Last Day\",",
              "    'DimTimeTable'[Date] > LastTradeDay, BLANK(),",
              "    'DimTimeTable'[DateKey]",
              ")"
            ]
