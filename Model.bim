            "dataType": "string",
            "isDataTypeInferred": true,
            "expression": [
              " ",
              "VAR LastTradeDay = CALCULATE(MAX('FactTradeDetails'[Date]), FILTER('DimTimeTable', 'DimTimeTable'[IsBeforeTheDayBeforeYesterday]))",
