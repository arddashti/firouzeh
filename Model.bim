          {
            "name": "Title - Fund Type",
            "expression": [
              "",
              "VAR NumToShow = 3",
              "VAR T1 =",
              "",
              "ADDCOLUMNS(",
              "    VALUES('DimFunds'[FundTypeEn]),",
              "    \"Rank\", ",
              "    RANKX(",
              "        VALUES('DimFunds'[FundTypeEn]),",
              "        [AUM],",
              "        ,",
              "        DESC",
              "    )",
              ")",
              "",
              "VAR T2 = ",
              "FILTER(",
              "    T1,",
              "    [Rank] <= NumToShow",
