              "\"Subscriptions T3 - \" & [Title - Fund Type] & \" - \" & [Title - Dates]"
            ]
          },
          {
            "name": "Title - Fund Sub Type",
            "expression": [
              "",
              "IF(",
              "    ISFILTERED('DimFunds'[FundSubType]),",
              "    \"(\" & CONCATENATEX(",
              "        VALUES('DimFunds'[FundSubTypeEn]),",
              "        'DimFunds'[FundSubTypeEn], ",
              "        \", \"",
              "    ) & \")\",",
              "    \"\"",
              ")",
              "",
              ""
            ]
          },
          {
